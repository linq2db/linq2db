<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<!--
		this file is added only as workaround to azure devops issue
		https://developercommunity.visualstudio.com/content/problem/983843/dotnet-build-task-does-not-use-nugetorg-for-one-pr.html
		and could be removed when issue fixed
		-->
		<add key="NuGet.org" value="https://api.nuget.org/v3/index.json" />

		<!-- we need a protobuf 1.0.162 or newer; at this time it's available only here. this feed can be removed in the future, when the required version will be available on nuget.org -->
		<add key="grpc-preview" value="https://www.myget.org/F/protobuf-net/api/v3/index.json" protocolVersion="3" />
	</packageSources>
</configuration>
