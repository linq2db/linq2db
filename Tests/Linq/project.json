        "dotnet-test-nunit": "3.4.0-beta-3",
        "xunit.core": "2.1.0"
      },
      "buildOptions": {
        "define": [ "NETSTANDARD", "NOFSHARP" ],
        "compile": {
          "exclude": [
            "**/AccessTests.cs",
            "**/DB2Tests.cs",
            "**/SybaseTests.cs",
            "**/SapHanaTests.cs",
            "**/MiniProfilerTests.cs",
            "**/FirebirdTests.cs",
            "**/InformixTests.cs",
            "**/DataServiceTests.cs",
            "**/AttributeReaderTests.cs",
            "**/XmlReaderTests.cs",
