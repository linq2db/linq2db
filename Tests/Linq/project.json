            "**/FSharpTests.cs",
            "**/AsyncTests.cs",
            "**/ParserTests.cs",
            "**/DB2Tests.cs",
            "**/InformixTests.cs"
          ]
        },
        "copyToOutput": {
          "mappings": {
            "x86/": { "include": "../../packages/System.Data.SQLite.Core.1.0.101.0/build/net40/x86/" },
            "x64/": { "include": "../../packages/System.Data.SQLite.Core.1.0.101.0/build/net40/x64/" },
            "Database/": {
              "include": "../../Data/"
            }
          }
