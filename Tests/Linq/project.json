        "Oracle.ManagedDataAccess": "12.1.24160419",
        "System.Data.SQLite.Core": "1.0.101.0",
        "xunit": "1.9.1",
        "xunit.extensions": "1.9.1",
        "NUnit.Runners": "3.6.0"

