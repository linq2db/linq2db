<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Configurations>Debug</Configurations>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<WarningLevel>9999</WarningLevel>
		<ErrorReport>prompt</ErrorReport>
		<Features>strict</Features>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
		<AccelerateBuildsInVisualStudio>true</AccelerateBuildsInVisualStudio>
		<ArtifactsPath>$(MSBuildThisFileDirectory)..\..\.build\T4.Nugets</ArtifactsPath>
		<ArtifactsPivots></ArtifactsPivots>
		<TestBinaries>$(MSBuildThisFileDirectory)..\..\.build\bin\Tests\Debug\net462\</TestBinaries>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="linq2db" />
	</ItemGroup>

</Project>
