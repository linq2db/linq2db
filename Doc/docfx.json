{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "src": "../Source"
        }
      ],
      "dest": "./api",
      "filter": "filterConfig.yml",      
      "properties": {
        "TargetFramework": "net45"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**/*.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      },
      {
        "files": [
          "/**/toc.yml",
          "/**/*.md"
        ],
        "exclude": [
          "doc/**",
          "packages/**"
        ],
        "src":"../",
        "dest": "articles"

      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**.png",
          "**.jpg"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "xref": [
      "../tools/msdn.4.5.2/content/msdn.4.5.2.zip"
    ],
    "globalMetadata": {
      "_appTitle": "Linq To DB (aka linq2db)",
      "_appFooter": "Copyright Â© 2011-2018 linq2db.com<br><br>Generated by <strong>DocFX</strong>",
      "_enableSearch": true,
      "_appLogoPath":"images/icon.png",
      "_appFaviconPath": "images/icon.ico"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false
  }
}