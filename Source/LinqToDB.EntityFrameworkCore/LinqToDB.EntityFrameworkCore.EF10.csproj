<Project Sdk="Microsoft.NET.Sdk">
	
	<Import Project="LinqToDB.EntityFrameworkCore.props" />

	<PropertyGroup>
		<TargetFrameworks>net10.0</TargetFrameworks>
		<Version>$(EF10Version)</Version>
		<DefineConstants>EF10;$(DefineConstants)</DefineConstants>
	</PropertyGroup>

	<PropertyGroup Label="Nuget">
		<PackageId>linq2db.EntityFrameworkCore</PackageId>
		<Title>Linq to DB extensions for Entity Framework Core 10</Title>
		<Description>Allows to execute Linq to DB queries in Entity Framework Core DbContext.</Description>
		<Product>$(Title)</Product>

		<PackageVersion>$(EF10Version)</PackageVersion>
		<PackageValidationBaselineVersion>10.0.0</PackageValidationBaselineVersion>

		<PackageTags>$(PackageTags);entity-framework-core;EntityFrameworkCore;EFCore</PackageTags>
	</PropertyGroup>

	<ItemGroup Label="Nuget">
		<None Include="../../Nuget/README.EF.md" Pack="true" PackagePath="readme.md" Visible="False" />
	</ItemGroup>

</Project>
