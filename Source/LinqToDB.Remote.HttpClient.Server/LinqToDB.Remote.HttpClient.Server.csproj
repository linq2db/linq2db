<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<AssemblyName>linq2db.Remote.HttpClient.Server</AssemblyName>
		<RootNamespace>LinqToDB.Remote.HttpClient.Server</RootNamespace>
		<OutputType>Library</OutputType>

		<TargetFrameworks>net8.0;net9.0;net10.0</TargetFrameworks>
		<TargetFrameworks Condition="'$(Configuration)'=='Testing'">net10.0</TargetFrameworks>

		<IsAotCompatible>true</IsAotCompatible>

		<Title>Linq to DB HttpClient Server Remote</Title>
		<Description>Linq to DB support for querying remote data contexts over HttpClient</Description>
		<Product>$(Title)</Product>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\LinqToDB\LinqToDB.csproj" />
	</ItemGroup>

	<ItemGroup>
		<AdditionalFiles Include="PublicAPI.*.txt" />
	</ItemGroup>
</Project>
