<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<AssemblyName>linq2db.Remote.Wcf</AssemblyName>
		<RootNamespace>LinqToDB.Remote.Wcf</RootNamespace>

		<DocumentationFile>$(ArtifactsPath)\bin\$(MSBuildProjectName)\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
		<TargetFrameworks>net462</TargetFrameworks>

		<Title>Linq to DB WCF Remote</Title>
		<Description>Linq to DB support for querying remote data contexts over WCF</Description>
		<Product>$(Title)</Product>
	</PropertyGroup>

	<ItemGroup>
		<AdditionalFiles Include="PublicAPI.*.txt" />
	</ItemGroup>
	
	<ItemGroup>
		<ProjectReference Include="..\LinqToDB\LinqToDB.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="System.ServiceModel" />
	</ItemGroup>
</Project>
