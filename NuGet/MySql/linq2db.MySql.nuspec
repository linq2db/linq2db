<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.MySql</id>
		<title>LINQ to MySql</title>
		<summary>
			This package includes a T4 template to generate data models for MySql database.
		</summary>
		<tags>linq linq2db MySql LinqToDB ORM database DB SQL</tags>
	</metadata>
	<files>
		<file src="t4bin\MySqlConnector.dll"                            target="tools"                                   />
		<file src="t4bin\System.Memory.dll"                             target="tools"                                   />
		<file src="t4bin\System.Buffers.dll"                            target="tools"                                   />
		<file src="t4bin\System.Threading.Tasks.Extensions.dll"         target="tools"                                   />
		<file src="t4bin\System.Diagnostics.DiagnosticSource.dll"       target="tools"                                   />
		<file src="t4bin\Microsoft.Extensions.Logging.Abstractions.dll" target="tools"                                   />

		<file src="NuGet\MySql\linq2db.MySql.props"                     target="build"                                   />
		<file src="NuGet\MySql\*.*"                exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\MySql\*.*"                exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
