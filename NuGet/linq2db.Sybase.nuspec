<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>linq2db.Sybase</id>
		<version>0.0.5</version>
		<title>LINQ to Sybase ASE</title>
		<authors>Igor Tkachev</authors>
		<owners />
		<licenseUrl>https://github.com/linq2db/linq2db/blob/master/MIT-LICENSE.txt</licenseUrl>
		<projectUrl>https://github.com/linq2db/linq2db</projectUrl>
		<iconUrl>http://www.gravatar.com/avatar/fc2e509b6ed116b9aa29a7988fdb8990?s=320</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>
			LINQ to Sybase ASE is a data access technology that provides a run-time infrastructure for managing relational data as objects.
			This package includes a T4 template to generate data models for Sybase database and references to the linq2db nugets.
		</description>
		<summary />
		<tags>linq linq2db Sybase LinqToDB ORM database DB</tags>
		<dependencies>
			<dependency id="linq2db"                   />
			<dependency id="linq2db.t4models"          />
		</dependencies>
	</metadata>
	<files>
		<file src="..\Redist\Sybase\Sybase.AdoNet2.AseClient.dll" target="tools" />
		<file src="..\Redist\Sybase\sybdrvado20.dll"              target="tools" />
		<file src="LinqToDB.Sybase.ttinclude"                     target="content\LinqToDB.Templates" />
		<file src="CopyMe.Sybase.tt.txt"                          target="content\LinqToDB.Templates" />
	</files>
</package>
