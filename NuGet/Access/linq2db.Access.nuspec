<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.Access</id>
		<title>LINQ to Access</title>
		<summary>
			This package includes a T4 template to generate data models for Access database and a reference to the linq2db nuget.
		</summary>
		<tags>linq linq2db Access LinqToDB ORM database DB SQL</tags>
	</metadata>
	<files>
		<file src="NuGet\Access\linq2db.Access.props"       target="build"                                   />
		<file src="NuGet\Access\*.*"   exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\Access\*.*"   exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
