<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.Oracle.Unmanaged</id>
		<title>LINQ to Oracle (ODP.NET) unmanaged</title>
		<summary>
			This package includes a T4 template to generate data models for Oracle database and references to the linq2db nuget.
			Oracle data provider for .NET will still need to be installed on the production or development machine in order to connect to Oracle.
		</summary>
		<tags>linq linq2db Oracle ODP LinqToDB ORM database DB SQL</tags>
		<dependencies>
			<dependency id="linq2db" version="6.0.0" />
		</dependencies>
		<contentFiles>
			<files include="**\*" buildAction="None" />
		</contentFiles>
	</metadata>
	<files>
		<file src="..\Source\LinqToDB\bin\Release\net46\linq2db.dll"                 target="tools"                                   />
		<file src="..\Tests\Linq\bin\Azure\net472\Humanizer.dll"                     target="tools"                                   />
		<file src="..\Tests\Linq\bin\Azure\net472\Oracle.ManagedDataAccess.dll"      target="tools"                                   />
		<file src="..\Tests\Linq\bin\Azure\net472\Microsoft.Bcl.AsyncInterfaces.dll" target="tools"                                   />

		<file src="..\..\NuGet\Oracle\linq2db.Oracle.props"                             target="build\linq2db.Oracle.Unmanaged.props"    />
		<file src="..\..\NuGet\Oracle\*.*"                         exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="..\..\NuGet\Oracle\*.*"                         exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
