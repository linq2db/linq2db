<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>linq2db.AspNet</id>
		<title>LINQ to DB ASP.NET Core Extensions</title>
		<description>LINQ to DB ASP.NET Core Extensions adds ability to configure ASP.NET Core application for Linq To DB usage.</description>
		<summary />
		<tags>
			linq linq2db LinqToDB aspnetcore ORM database DB SQL SqlServer Access SqlCe SqlServerCe MySql Firebird SQLite Sybase Oracle ODP PostgreSQL DB2 Informix SapHana
		</tags>
		<dependencies>
			<group targetFramework=".NETFramework4.5" >
				<dependency id="linq2db" version="3.0.0"/>
				<dependency id="Microsoft.Extensions.DependencyInjection" version="1.1.1"/>
				<dependency id="Microsoft.Extensions.Logging.Abstractions" version="1.1.2"/>
			</group>
			<group targetFramework=".NETStandard2.0">
				<dependency id="linq2db" version="3.0.0"/>
				<dependency id="Microsoft.Extensions.DependencyInjection" version="3.1.5"/>
				<dependency id="Microsoft.Extensions.Logging.Abstractions" version="3.1.5"/>
			</group>
		</dependencies>
	</metadata>

	<files>
		<file src="..\Source\LinqToDB.AspNet\bin\Release\**\linq2db.AspNet.pdb" target="lib\" />
		<file src="..\Source\LinqToDB.AspNet\bin\Release\**\linq2db.AspNet.xml" target="lib\" />
		<file src="..\Source\LinqToDB.AspNet\bin\Release\**\linq2db.AspNet.dll" target="lib\" />
	</files>
</package>
