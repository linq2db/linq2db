<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.Oracle</id>
		<title>LINQ to Oracle</title>
		<summary>
			This package includes a T4 template to generate data models for Oracle database.
		</summary>
		<tags>linq linq2db Oracle LinqToDB ORM database DB SQL</tags>
	</metadata>
	<files>
		<file src="t4bin\Oracle.ManagedDataAccess.dll"    target="tools"                                   />

		<file src="NuGet\Oracle\linq2db.Oracle.props"     target="build\linq2db.Oracle.props"              />
		<file src="NuGet\Oracle\*.*" exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\Oracle\*.*" exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
