<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.ClickHouse.Client</id>
		<title>LINQ to ClickHouse (ClickHouse.Client)</title>
		<summary>
			This package includes a T4 template to generate data models for ClickHouse database and references to the linq2db and ClickHouse.Client nugets.
		</summary>
		<tags>linq linq2db ClickHouse LinqToDB ORM database DB SQL</tags>
		<readme>README.md</readme>
		<dependencies>
			<dependency id="linq2db"           version="5.0.0" />
			<dependency id="ClickHouse.Client" version="6.5.2" />
		</dependencies>
		<contentFiles>
			<files include="**\*" buildAction="None" />
		</contentFiles>
	</metadata>
	<files>
		<file src="t4bin\linq2db.dll"                             target="tools"                                   />
		<file src="t4bin\Humanizer.dll"                           target="tools"                                   />
		<file src="t4bin\MySqlConnector.dll"                      target="tools"                                   />
		<file src="t4bin\System.Memory.dll"                       target="tools"                                   />
		<file src="t4bin\System.Buffers.dll"                      target="tools"                                   />
		<file src="t4bin\System.Threading.Tasks.Extensions.dll"   target="tools"                                   />
		<file src="t4bin\System.Diagnostics.DiagnosticSource.dll" target="tools"                                   />
		<file src="t4bin\Microsoft.Bcl.AsyncInterfaces.dll"       target="tools"                                   />

		<file src="NuGet\ClickHouse\linq2db.ClickHouse.props"     target="build\linq2db.ClickHouse.Client.props"   />
		<file src="NuGet\ClickHouse\*.*"     exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\ClickHouse\*.*"     exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
