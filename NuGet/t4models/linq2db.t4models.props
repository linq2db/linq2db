<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<!--linq2db.dll path-->
		<LinqToDBT4SharedTools>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4SharedTools>

		<!--client assemblies pathes-->
		<LinqToDBT4FirebirdClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4FirebirdClientPath>
		<LinqToDBT4MySqlConnectorClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4MySqlConnectorClientPath>
		<LinqToDBT4OracleManagedClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4OracleManagedClientPath>
		<LinqToDBT4NpgsqlClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4NpgsqlClientPath>
		<LinqToDBT4SQLiteClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4SQLiteClientPath>
		<LinqToDBT4SQLServerClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4SQLServerClientPath>
		<LinqToDBT4SybaseDataActionClientPath>$(MSBuildThisFileDirectory)..\tools\</LinqToDBT4SybaseDataActionClientPath>

		<!--templates pathes-->
		<LinqToDBT4AccessTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4AccessTemplatesPath>
		<LinqToDBT4DB2TemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4DB2TemplatesPath>
		<LinqToDBT4FirebirdTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4FirebirdTemplatesPath>
		<LinqToDBT4InformixTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4InformixTemplatesPath>
		<LinqToDBT4MySqlTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4MySqlTemplatesPath>
		<LinqToDBT4OracleTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4OracleTemplatesPath>
		<LinqToDBT4PostgreSQLTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4PostgreSQLTemplatesPath>
		<LinqToDBT4SapHanaTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4SapHanaTemplatesPath>
		<LinqToDBT4SqlCeTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4SqlCeTemplatesPath>
		<LinqToDBT4SQLiteTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4SQLiteTemplatesPath>
		<LinqToDBT4SqlServerTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4SqlServerTemplatesPath>
		<LinqToDBT4SybaseAseTemplatesPath>$(MSBuildThisFileDirectory)..\contentFiles\any\any\LinqToDB.Templates\</LinqToDBT4SybaseAseTemplatesPath>
	</PropertyGroup>
</Project>
