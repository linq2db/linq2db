<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.Access</id>
		<title>LINQ to Access</title>
		<summary>
			This package includes a T4 template to generate data models for Access database and a reference to the linq2db nuget.
		</summary>
		<tags>linq linq2db Access LinqToDB ORM database DB SQL</tags>
		<readme>README.md</readme>
		<dependencies>
			<group targetFramework="net462">
				<dependency id="linq2db"           version="6.0.0" />
			</group>
			<group targetFramework="netstandard2.0">
				<dependency id="linq2db"           version="6.0.0" />
				<dependency id="System.Data.OleDb" version="6.0.0" />
			</group>
			<group targetFramework="net6.0">
				<dependency id="linq2db"           version="6.0.0" />
				<dependency id="System.Data.OleDb" version="6.0.0" />
			</group>
			<group targetFramework="net7.0">
				<dependency id="linq2db"           version="6.0.0" />
				<dependency id="System.Data.OleDb" version="7.0.0" />
			</group>
			<group targetFramework="net8.0">
				<dependency id="linq2db"           version="6.0.0" />
				<dependency id="System.Data.OleDb" version="8.0.0" />
			</group>
		</dependencies>
		<contentFiles>
			<files include="**\*" buildAction="None" />
		</contentFiles>
	</metadata>
	<files>
		<file src="t4bin\linq2db.dll"                       target="tools"                                   />
		<file src="t4bin\Humanizer.dll"                     target="tools"                                   />
		<file src="t4bin\Microsoft.Bcl.AsyncInterfaces.dll" target="tools"                                   />

		<file src="NuGet\Access\linq2db.Access.props"       target="build"                                   />
		<file src="NuGet\Access\*.*"   exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\Access\*.*"   exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
