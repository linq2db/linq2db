	<metadata>
		<id>linq2db.Access</id>
		<version>2.0.0-rc</version>
		<title>LINQ to Access</title>
		<authors>Igor Tkachev, Ilya Chudin, Svyatoslav Danyliv, Dmitry Lukashenko</authors>
