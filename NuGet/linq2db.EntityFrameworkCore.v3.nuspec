<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>linq2db.EntityFrameworkCore</id>
		<title>Linq to DB extensions for Entity Framework Core 3.1</title>
		<description>Allows to execute Linq to DB queries in Entity Framework Core DbContext.</description>
		<tags>entity-framework-core EntityFrameworkCore EFCore</tags>
		<dependencies>
			<group targetFramework="netstandard2.0">
				<dependency id="Microsoft.EntityFrameworkCore.Relational" version="3.1.0" />
				<dependency id="linq2db"                                  version="6.0.0" />
			</group>
		</dependencies>
	</metadata>

	<files>
		<file src="NuGet\README.EF.md"                                                                  target="README.md"           />

		<file src="bin\LinqToDB.EntityFrameworkCore\{0}\netstandard2.0\linq2db.EntityFrameworkCore.pdb" target="lib\netstandard2.0\" />
		<file src="bin\LinqToDB.EntityFrameworkCore\{0}\netstandard2.0\linq2db.EntityFrameworkCore.xml" target="lib\netstandard2.0\" />
		<file src="bin\LinqToDB.EntityFrameworkCore\{0}\netstandard2.0\linq2db.EntityFrameworkCore.dll" target="lib\netstandard2.0\" />
	</files>
</package>
