<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.Firebird</id>
		<title>LINQ to Firebird</title>
		<summary>
			This package includes a T4 template to generate data models for Firebird database.
		</summary>
		<tags>linq linq2db Firebird LinqToDB ORM database DB SQL</tags>
	</metadata>
	<files>
		<file src="t4bin\FirebirdSql.Data.FirebirdClient.dll" target="tools"                                   />

		<file src="NuGet\Firebird\linq2db.Firebird.props"     target="build"                                   />
		<file src="NuGet\Firebird\*.*"   exclude="**\*.props" target="contentFiles\any\any\LinqToDB.Templates" />
		<file src="NuGet\Firebird\*.*"   exclude="**\*.props" target="content\LinqToDB.Templates"              />
	</files>
</package>
