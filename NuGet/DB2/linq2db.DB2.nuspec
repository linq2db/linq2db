<?xml version="1.0"?>
<!-- %T4% -->
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.3.0">
		<id>linq2db.DB2</id>
		<title>LINQ to IBM DB2</title>
		<summary>
			This package includes a T4 template to generate data models for DB2 database.
			IBM Data Server Provider for .NET will still need to be installed on the production or development machine in order to connect to DB2.
		</summary>
	</metadata>
	<files>
		<file src="t4bin\IBM.Data.DB2.DLL" target="tools"           />
		<file src="t4bin\clidriver\**"     target="tools\clidriver" />
	</files>
</package>
